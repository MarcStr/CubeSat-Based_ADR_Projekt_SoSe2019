      GMAT 'Reset ECC' TestSat.Earth.ECC = 0.005;
      GMAT 'Reset Date' TestSat.A1ModJulian = 27204.99998882387;
      Report 'StartStats' EllipticDeorbit TestSat.Busek_BHT_200_largetank.FuelMass TestSat.ElapsedDays TestSat.Earth.RadApo TestSat.Earth.RadPer TestSat.Earth.SMA TestSat.Earth.ECC TestSat.TotalMass orbit_height target_mass;
      While 'While h>180km or t<25y or fuel>0kg' TestSat.A1ModJulian < 37251 & TestSat.Busek_BHT_200_largetank.FuelMass > 0.001 & TestSat.Earth.RMAG >= 6558 & Abbruch == 0 & Abbruch == 0
         %        GMAT 'Abbruch->0' Abbruch = 0;
         Propagate 'PropToBurnStart' PD78_Acc11_PMOff(TestSat) {TestSat.Earth.TA = 151, StopTolerance = 1e-005, OrbitColor = [72 254 46]};
         BeginFiniteBurn 'BurnStart' FiniteBurn(TestSat);
         Propagate 'PropToBurnEnd' PD78_Acc11_PMOff(TestSat) {TestSat.Earth.TA = 209, TestSat.Busek_BHT_200_largetank.FuelMass = 0, TestSat.ElapsedSecs = 1200, StopTolerance = 1e-005, OrbitColor = [0 236 255]};
         EndFiniteBurn 'BurnEnd' FiniteBurn(TestSat);
         If TestSat.Earth.RadPer >= 6557
            Propagate 'Prop to Periapsis' PD78_Acc11_PMOff(TestSat) {TestSat.Earth.Periapsis};
         EndIf;
         
         If TestSat.Earth.RadPer < 6557
            GMAT 'Abbruch Per' Abbruch = 1;
         EndIf;
         If TestSat.Earth.ECC < 0.00075
            GMAT 'Abbruch ECC' Abbruch = 2;
         EndIf;
         
         Report 'EndWhile Report' EllipticDeorbit TestSat.Busek_BHT_200_largetank.FuelMass TestSat.ElapsedDays TestSat.Earth.RadApo TestSat.Earth.RadPer TestSat.Earth.SMA TestSat.Earth.ECC TestSat.TotalMass orbit_height target_mass;
      EndWhile;
      Report 'EndStats' EllipticDeorbit TestSat.Busek_BHT_200_largetank.FuelMass TestSat.ElapsedDays TestSat.Earth.RadApo TestSat.Earth.RadPer TestSat.Earth.SMA TestSat.Earth.ECC TestSat.TotalMass orbit_height target_mass;
      Report 'AWReport' AwReport TestSat.Busek_BHT_200_largetank.FuelMass TestSat.ElapsedDays TestSat.Earth.RadApo TestSat.Earth.RadPer TestSat.Earth.SMA TestSat.Earth.ECC TestSat.TotalMass orbit_height target_mass Abbruch;
   EndFor;
EndFor;









